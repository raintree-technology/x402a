# x402a Configuration
# Facilitator is the account that pays gas fees
x402_facilitator_private_key=<YOUR_PRIVATE_KEY>
x402_facilitator_address=<YOUR_FACILITATOR_ADDRESS>
contract_address=<YOUR_CONTRACT_ADDRESS>

# Shelby Configuration (x402s)
# Get API key from: https://geomi.dev
SHELBY_API_KEY=<GET_FROM_GEOMI_DEV>
SHELBY_NETWORK=SHELBYNET
# Payment recipient (where x402s payments go)
SHELBY_PAYMENT_RECIPIENT=<YOUR_ADDRESS>

# Pricing: How many octas (10^-8 APT) per chunkset
# Default: 100000 = 0.001 APT per chunkset
SHELBY_OCTAS_PER_CHUNKSET=100000
