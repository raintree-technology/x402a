{
  "name": "x402a",
  "version": "0.2.1",
  "description": "Experimental x402 payment protocol implementation for Aptos blockchain - Supports APT Coin and Fungible Assets - client, server, and React in one package",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts src/server.ts src/client.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "format": "biome format src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "x402",
    "x402a",
    "aptos",
    "payments",
    "micropayments",
    "blockchain",
    "move",
    "cryptocurrency",
    "react",
    "hooks",
    "facilitator",
    "server"
  ],
  "author": "Raintree Technology <support@raintree.technology>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raintree-technology/x402a.git"
  },
  "bugs": {
    "url": "https://github.com/raintree-technology/x402a/issues"
  },
  "homepage": "https://github.com/raintree-technology/x402a#readme",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.30.0",
    "@aptos-labs/wallet-adapter-react": "^3.7.2",
    "pino": "^10.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@vitest/coverage-v8": "^1.0.4",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "@aptos-labs/wallet-adapter-react": ">=3.0.0",
    "react": ">=18.0.0"
  }
}
